<!DOCTYPE html>
<html>
<head>
    <title>Dashboard</title>
    <!-- Add your CSS styles here -->
</head>
<body>
    <!-- Navbar (already implemented) -->
    <!-- Main content -->
    <div class="dashboard-container">
        <div class="header">
            <h1>September 2023</h1>
        </div>
        <div class="summary">
            <div class="total-costs">
                <div class="cleaning-cost">
                    <div class="rounded-rectangle">
                        <p>Total Cleaning Costs:</p>
                        <p>$500</p> <!-- Replace with actual cost -->
                    </div>
                </div>
                <div class="expenses-cost">
                    <div class="rounded-rectangle">
                        <p>Total Expenses:</p>
                        <p>$200</p> <!-- Replace with actual cost -->
                    </div>
                </div>
            </div>
        </div>
        <div class="cleaning-jobs">
            <div class="rounded-rectangle">
                <table>
                    <thead>
                        <tr>
                            <th>Property</th>
                            <th>Cleaner</th>
                            <th>Date</th>
                            <th>Price</th>
                            <th>Expenses</th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- Populate with cleaning job data -->
                    </tbody>
                </table>
            </div>
        </div>
        <div class="my-team">
            <div class="rounded-rectangle">
                <table>
                    <thead>
                        <tr>
                            <th>Cleaner Name</th>
                            <th>Cleanings</th>
                            <th>Average Price</th>
                            <th>Rating</th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- Populate with team member data -->
                    </tbody>
                </table>
            </div>
        </div>
        <div class="my-property">
            <div class="rounded-rectangle">
                <h2>My Property</h2>
                <table>
                    <tr>
                        <td>Property Name:</td>
                        <td>Sample Property</td> <!-- Replace with actual property name -->
                    </tr>
                    <tr>
                        <td>Address:</td>
                        <td>123 Main St, City</td> <!-- Replace with actual address -->
                    </tr>
                    <tr>
                        <td>Number of Reviews:</td>
                        <td>50</td> <!-- Replace with actual number of reviews -->
                    </tr>
                    <tr>
                        <td>Check-in Time:</td>
                        <td>3:00 PM</td> <!-- Replace with actual check-in time -->
                    </tr>
                    <tr>
                        <td>Check-out Time:</td>
                        <td>11:00 AM</td> <!-- Replace with actual check-out time -->
                    </tr>
                    <tr>
                        <td>Co-owners:</td>
                        <td>John Doe, Jane Smith</td> <!-- Replace with actual co-owners -->
                    </tr>
                </table>
                <!-- Add property image here -->
            </div>
        </div>
        <div class="monthly-costs">
            <div class="rounded-rectangle">
                <h2>Monthly Costs</h2>
                <p>Total Costs: $700</p> <!-- Replace with actual total monthly costs -->
            </div>
        </div>
    </div>
</body>
</html>
